#include <std.h>
#include <terrain.h>
#include "../../tecqumin.h"
inherit J_MAZE;

void create() {
  ::create();
  set_exits( ([ "north": JUNG_ROOM + "j_link6", "northwest" : ROOM + "rightbank4"]) );
}
void init(){
  object mazeroom;
  string *exits;
  ::init();
  mazeroom = find_object_or_load(JUNG_ROOM6 + "j_maze0.c");
  exits = mazeroom->query_exits();
  if (sizeof(query_exits())<3){
    BUILDER->restore_exits(TO);
  }
  if (sizeof(exits)<3{
    BUILDER->restore_exits(mazeroom);
    exits = mazeroom->query_exits();
  }
  if (sizeof(exits)<3){

    "/daemon/maze_d.c"->set_roompath(JUNG_ROOM6 + "j_maze");
    "/daemon/maze_d.c"->set_southroom(JUNG_ROOM + "j_link6.c");
    "/daemon/maze_d.c"->set_northroom(JUNG_ROOM + "j_link5a.c");
    "/daemon/maze_d.c"->set_westroom(ROOM + "rightbank3.c");
    "/daemon/maze_d.c"->set_special_rooms( ({  }) );
    "/daemon/maze_d.c"->init_maze(5, 5, JUNG_ROOM6 + "j_maze", 30);
  }   
}