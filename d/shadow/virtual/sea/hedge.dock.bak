#include <std.h>

inherit DOCK;

int is_virtual()
{
  return 1;
}

int is_water()
{
  return 1;
}

int is_dock()
{
  return 1;
}

void create()
{
    ::create();
    set_indoors(1);
    set_light(2);
    set_short("Hedge Field Docks");
    set_long("You stand before a island of hedge, moving in the wind. A glaze "
