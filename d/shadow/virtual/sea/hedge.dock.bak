	     "into it, and it awaits you.");
    set_exits( ([
		"south" : "/d/hm_quest/enterence"
	     ]) );
    set_smell("default","You smell the wonderful sent of freshly air of a "
	      "garden, right after a light rain.");
    set_listen("default","You hear the sounds of waves crashing against the "
	       "sandy beaches."); 
//    find_object_or_load("d/shadow/virtual/sea/obj/ferryboat1")->move(TO);
}
